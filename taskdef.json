{
  "family": "laravel-app",
  "executionRoleArn": "arn:aws:iam::727646469309:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "laravel-app",
      "image": "<IMAGE1_NAME>",
      "essential": true,
      "memory": 512,
      "cpu": 256
    },
    {
      "name": "laravel-node",
      "image": "<IMAGE2_NAME>",
      "essential": true,
      "memory": 512,
      "cpu": 256
    },
    {
      "name": "laravel-nginx",
      "image": "<IMAGE3_NAME>",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ]
    }
  ]
}
